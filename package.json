{
  "name": "ts-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx use-yarn",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "commit": "git-cz",
    "test": "jest -u",
    "test:watch": "jest --watch",
